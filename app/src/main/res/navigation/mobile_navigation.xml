<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.dispenser.ui.home.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dispenser"
        android:name="com.example.dispenser.ui.home.DispenserFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_dispenser" />
    <fragment
        android:id="@+id/navigation_schedule"
        android:name="com.example.dispenser.ui.schedule.list_schedule.ScheduleListFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_schedule_list"/>
    <fragment
        android:id="@+id/navigation_calibration"
        android:name="com.example.dispenser.ui.calibration.CalibrationFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_calibration"/>


    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.dispenser.ui.home.ProfileFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_profile" />
</navigation>